<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Output</title>
  </head>
  <body>
    <h1>Results Page</h1>
    <div id="saved-restaurants">
      <table style="width=100%">
        <tr>
          <th>Name</th>
          <th>Rating</th>
          <th>City</th>
          <th>Cuisine Type</th>
        </tr>
        @foreach(var restaurant in Model)
        {
          <tr>
            <td>@restaurant.GetName()</td>
            <td>@restaurant.GetRating()</td>
            <td>@restaurant.GetCity()</td>
            <td>@restaurant.GetCuisineName()</td>
          </tr>
        }
      </table>

    </div>
  </body>
</html>
